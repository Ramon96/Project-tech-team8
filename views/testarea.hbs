<h1> Get</h1>
<form action="/testarea/get-data" method="GET">
<button type="submit">Get</button>
{{!-- <a href="/testarea/get-data">Get data</a> --}}
{{#each items}}
    <div>id: {{this.id}} </div>
    <div>username: {{this.username}} </div>
    <div>profilepic: {{this.profilePicture}} </div>
    <div>premium: {{this.premium}} </div>
    <div>aboutme: {{this.aboutme}} </div>
    <div>function: {{this.function}} </div>
    <div>company: {{this.company}} </div>
    <div>city: {{this.city}} </div>
    <div>isMale: {{this.isMale}} </div>
    <div>prefMen: {{this.prefMen}} </div>
    <div>email: {{this.email}} </div>
    <div>minAge: {{this.minAge}} </div>
    <div>maxAge: {{this.maxAge}} </div>
    <div>maxDist: {{this.maxDistance}} </div>
    <div>DoB: {{this.DoB}} </div>
    <div>pushNotification: {{this.pushNotification}} </div>

{{/each}}

